{
  "name": "express-graphql-api",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/main.js",
    "dev": "nodemon src/main.ts",
    "add-migration": "npx prisma migrate dev",
    "update-database": "npx prisma db push",
    "seed-db": "npx prisma db seed"
  },
  "private": true,
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@ngneat/falso": "^7.2.0",
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.2",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
